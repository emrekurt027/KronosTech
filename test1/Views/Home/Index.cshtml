
@{
    ViewData["Title"] = "Homes";
}

<style>
    #mainSlider {
        height: 90vh;
    }

    #mainSlider-List > .carousel-item > img {
        opacity: .9;
        background: black;
    }

    #mainSlider-List > .carousel-item {
        height: 90vh;
        background: no-repeat center center;
        -webkit-background-size: cover;
        -moz-background-size: cover;
        -o-background-size: cover;
        background-size: cover;
    }

    #mainSlider-List > .carousel-caption {
        background: rgba(0,0,0,.7);
        opacity: .9;
        position: sticky;
    }

    .my-card {
        position: absolute;
        left: 40%;
        top: -20px;
        border-radius: 50%;
        width: 20%;
    }
</style>

<div id="mainSlider" class="carousel slide w-100 h-100" style="" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#mainSlider" data-slide-to="0" class="active"></li>
        @for (int i = 1; i < ViewBag.appFieldList.Count; i++)
        {
            <li data-target="#mainSlider" data-slide-to="@i"></li>
        }

        <li data-target="#mainSlider" data-slide-to="@ViewBag.appFieldList.Count"></li>
    </ol>
    <div class="carousel-inner" id="mainSlider-List">
        <div class="carousel-item active">
            <img src="~/lib/images/app/@ViewBag.appFieldList[0].path/@ViewBag.appFieldList[0].images[0]" class="h-100 w-100">
            <div class="carousel-caption d-sm-flex flex-column d-md-block animated slideInRight">
                <h2 class="flex-grow-1">@ViewBag.appFieldList[0].name</h2>
                <p class="flex-grow-1">deneme</p>
                <div><a href="/@SharedHtmlLocalizer["Menu.Application"]/@ViewBag.appFieldList[0].name" class="nav-item p-2" style="text-decoration:none;border:1px;border-radius:5px;">Daha Fazlası İçin</a></div>
            </div>
        </div>
        @for (int i = 1; i < ViewBag.appFieldList.Count; i++)
        {
            <div class="carousel-item">
                <img src="~/lib/images/app/@ViewBag.appFieldList[i].path/@ViewBag.appFieldList[i].images[0]" class="h-100 w-100" style="max-height:600px; opacity:.5;">
                <div class="carousel-caption d-sm-flex flex-column d-md-block animated slideInRight">
                    <h2 class="flex-grow-1">@ViewBag.appFieldList[i].name</h2>
                    <p class="flex-grow-1">deneme</p>
                    <div><a href="/@SharedHtmlLocalizer["Menu.Application"]/@ViewBag.appFieldList[i].name" class="nav-item p-2" style="text-decoration:none;border:1px;border-radius:5px;">Daha Fazlası İçin</a></div>
                </div>
            </div>
        }

        <div class="carousel-item ">
            <img src="~/lib/images/arıtma.png" class="h-100 w-100">
            <div class="carousel-caption d-sm-flex flex-column fa-pull-right d-md-block animated slideInDown">
                <h2 class="flex-grow-1">Taşıt Yıkama için Arıtma Sistemleri</h2>
                <p class="flex-grow-1">
                    Besleme suyunun arıtılması ise %99 renksiz, kokusuz ve lekesiz su ile araçlarınızın kaporta camlarında lekeler kalmaması için üretiyoruz.
                </p>
                <div><a href="#" class="nav-item p-2" style="text-decoration:none;border:1px;border-radius:5px;">Daha Fazlası İçin</a></div>
            </div>
        </div>


    </div>
    <a class="carousel-control-prev nav-item" href="#mainSlider" role="button" data-slide="prev">
        <i class="fas fa-chevron-circle-left fa-slider"></i>
    </a>
    <a class="carousel-control-next nav-item" href="#mainSlider" role="button" data-slide="next">
        <i class="fas fa-chevron-circle-right fa-slider"></i>
    </a>
</div>

<style>
    .tab-pane {
    }

    .g-recaptcha {
        transform-origin: left top;
        -webkit-transform-origin: left top;
    }
</style>

